

<!-- ディスク新規追加ページ -->

<div>
  <h1>ディスク新規追加ページ</h1>

  <%= form_for(@disc) do |f| %>
    <h4>ディスク名</h4>
    <%= f.text_field :disc_name %>
    <h4>曲名</h4>
    <%= f.fields_for :song_titles do |ff| %>
      <%= ff.text_field :song_title %>
    <% end %>
    <br>
    <%= f.submit "Create Disc" %>
  <% end %>
</div>