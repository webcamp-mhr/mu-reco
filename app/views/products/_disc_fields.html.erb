
<!-- ディスク新規追加ページ -->

<div class="nested-fields">
  <h4>ディスク名</h4>
  <%= f.text_field :disc_name %><br>
  <%= link_to_remove_association "ディスク削除", f %>
</div>

<div>
  <h4>曲</h4>
  <%= f.fields_for :song_titles do |song_title| %>
    <%= render 'song_title_fields', f: song_title %>
  <% end %>
  <div>
    <%= link_to_add_association "曲名追加", f, :song_titles %>
  </div>
</div>
